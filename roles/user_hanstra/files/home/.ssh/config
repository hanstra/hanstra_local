Host *
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes


Host ndlib
  Hostname github.com
  User git
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa

Host ndlibrary
  Hostname github.com
  User git
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519

Host hanstra
  Hostname github.com
  User git
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
