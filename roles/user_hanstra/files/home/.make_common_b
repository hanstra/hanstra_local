#set history=32
CDPATH=$CDPATH:..:~:/usr/local/bin:/usr/local/src
export CDPATH
SQLPATH=.:/global/soft/aleph/sql:/global/esuadmin/oracle/scripts
export SQLPATH
JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_25.jdk/Contents/Home
export JAVA_HOME
EC2_HOME=/Users/hanstra/ec2
export EC2_HOME
EDITOR=vi
export EDITOR
TERM=xterm
export TERM
EXINIT='set aw sw=3 ts=3 report=3 
               map v xp\
               map ; kJ\
               set bf'
export EXINIT
stty echoe
stty erase 
stty cols 120
unset autologout
export LSCOLORS=gxBxhxDxfxhxhxhxhxcxcx

